{
  "compilerOptions": {
    "removeComments": false,
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "declaration": true,
    "sourceMap": true,
    "suppressImplicitAnyIndexErrors": true,
    "module": "commonjs",
    "target": "ES2015",
    "rootDir": "./",
    "outDir": "test/out",
    "skipLibCheck": true,
    "lib": ["dom", "es5", "scripthost", "es2015"],
    "jsx": "react",
    "jsxFactory": "JsxFactory.createElement"
  },
  "include": ["../src/*", "./**/*"]
}

{
  "name": "@cycle/react-dom",
  "version": "2.2.0",
  "description": "Cycle.js driver that uses React DOM to render the view",
  "author": "Andre Staltz <contact@staltz.com>",
  "license": "MIT",
  "bugs": "https://github.com/cyclejs/react-dom/issues",

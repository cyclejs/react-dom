    "react-dom",
    "cyclejs",
    "xstream",
    "mvi",
    "react-native",
    "driver"
  ],
  "main": "lib/cjs/index.js",
  "typings": "lib/cjs/index.d.ts",
  "types": "lib/cjs/index.d.ts",
  "peerDependencies": {
    "@cycle/react": "^2.2.0",
    "react": ">=16.4.x",
    "react-dom": ">=16.4.x",
    "xstream": "11.x.x"
  },
  "devDependencies": {
    "@cycle/run": "^5.1.0",
    "@types/mocha": "^2.2.40",
    "@types/node": "^10.5.2",
    "@types/react": "16.4.x",
    "@types/react-dom": "16.x.x",
    "karma": "2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-typescript": "^3.0.12",
    "karma-typescript-es6-transform": "^1.0.4",
    "mocha": "^5.2.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.4.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npm run compile",
